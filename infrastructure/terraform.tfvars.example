# Copy this file to terraform.tfvars and fill in your values

# Scaleway credentials
scaleway_access_key      = "your-access-key"
scaleway_secret_key      = "your-secret-key"
scaleway_organization_id = "your-organization-id"
scaleway_project_id      = "your-project-id"

# Domain configuration
domain_name = "motord.eu"

# Region configuration (optional, defaults to fr-par)
region = "fr-par"
zone   = "fr-par-1" 